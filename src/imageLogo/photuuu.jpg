var flag = 1;
$('#success_message').html('');
if(!name)
{
	$('input[name="name"]').css("border-color", "red");
	flag = 0;
}
if(!email)
{
	$('input[name="email"]').css("border-color", "red");
	flag = 0;
}
if(!contactnumber)
{
	$('input[name="contactnumber"]').css("border-color", "red");
	flag = 0;
}
