		$('input[name="name"]').val('');
		$('input[name="email"]').val('');
		$('textarea[name="enquiry"]').val('');
		$('input[name="contactnumber"]').val('');
	    },
        error: function(err) {
        alert(err);
        }
      });	 	
}
}
